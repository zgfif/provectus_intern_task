<h1>New event</h1>
<p><strong>Place for form</strong></p>
<%= form_with url: events_path do |form| %>
  <p>
  	<%= form.label :title %> 
  	<%= form.text_field :title %>
  </p>

  <p>
  	<%= form.label :tag %> 
  	<%= form.text_field :tag %>		
  </p>

  <p>
  	<%= form.label :scheduled_date %> 
  	<%= form.text_field :scheduled_date %> 	
  </p>
  <p><%= form.submit %></p>
<% end %>
<%= link_to 'Back', :back %>